<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<HBox alignment="CENTER" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.202-ea" xmlns:fx="http://javafx.com/fxml/1">
<children>
    <VBox>
        <children>
            <ListView fx:id="userList">
            </ListView>
        </children>
    </VBox>
    <VBox alignment="CENTER">
        <children>
            <Button fx:id="updateButton" mnemonicParsing="false" onAction="#update" text="Update">
                <VBox.margin>
                    <Insets right="10.0" />
                </VBox.margin>
            </Button>
            <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#delete" text="Delete" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancel" text="Cancel" />

        </children>
        <VBox.margin>
            <Insets top="15.0" />
        </VBox.margin>
    </VBox>
</children>
<padding>
    <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
</padding>
</HBox>
